<!DOCTYPE html>
<html ng-app="app">
  <head>
    <title>BTC Quot</title>
    <link rel="stylesheet" type="text/css" href="/css/app.css" media="all" />
  </head>
  <body ng-controller="TradeController">
    <div class="row">
      <div class="large-12">
        <h1>BTC Quot</h1>
        <table style="margin: auto">
            <tr>
                <th>市场</th>
                <th>最新价</th>
                <th>买入价</th>
                <th>卖出价</th>
                <th>最高价</th>
                <th>最低价</th>
                <th>成交量</th>
            </tr>
            <tr>
                <td>BTC China</td>
                <td ng-bind="btcChinaTrade.ticker.last"></td>
                <td ng-bind="btcChinaTrade.ticker.buy"></td>
                <td ng-bind="btcChinaTrade.ticker.sell"></td>
                <td ng-bind="btcChinaTrade.ticker.high"></td>
                <td ng-bind="btcChinaTrade.ticker.low"></td>
                <td ng-bind="btcChinaTrade.ticker.vol"></td>
            </tr>
            <tr>
                <td>FXBTC</td>
                <td ng-bind="fxBtcTrade.ticker.last_rate"></td>
                <td ng-bind="fxBtcTrade.ticker.bid"></td>
                <td ng-bind="fxBtcTrade.ticker.ask"></td>
                <td ng-bind="fxBtcTrade.ticker.high"></td>
                <td ng-bind="fxBtcTrade.ticker.low"></td>
                <td ng-bind="fxBtcTrade.ticker.vol"></td>
            </tr>
        </table>
      </div>
    </div>

    <script type="text/javascript" src="/js/app.js"></script>
  </body>
</html>
